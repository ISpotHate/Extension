{
    "manifest_version": 2,
    "name": "HateSpotter",
    "version": "1.0",
    "description": "This is a Chrome extension that replaces hate speech.",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js", "jquery.min.js"] 
    }],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]
}